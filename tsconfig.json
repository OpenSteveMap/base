{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": ".",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "declaration": true,
    "sourceMap": true
  },
  "exclude": [
    "node_modules",
    "typings",
    "ts"
  ]
}
